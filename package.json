{
	"name": "switch-to-spec",
	"displayName": "Switch to spec",
	"description": "Switch between spec and code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
    "test",
    "spec",
    "jest",
    "rspec",
		"switch",
		"ruby",
		"javascript"
  ],
	"activationEvents": [
		"onCommand:extension.switchToSpec"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.switchToSpec",
			"title": "Switch to spec"
		}],
    "keybindings": [{
			"command": "extension.switchToSpec",
			"key": "ctrl+shift+t",
			"mac": "cmd+shift+t",
			"when": "editorTextFocus"
		}]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
